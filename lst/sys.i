    *((volatile unsigned int *)(_mRegAddr)) = ((*((volatile unsigned int *)(_mRegAddr))) & ~(0x07 << shift)) | (uIntLevel << shift);

    return 0;
}


INT32 sysSetInterruptType(IRQn_Type eIntNo, UINT32 uIntSourceType)
{
    UINT32 _mRegAddr;
    INT     shift;

    if ((eIntNo > 62) || (eIntNo < 1))
        return 1;

    _mRegAddr = (0xB8002000+0x00) + ((eIntNo / 4) * 4);
    shift = (eIntNo % 4) * 8;
    uIntSourceType &= 0xC0;
    *((volatile unsigned int *)(_mRegAddr)) = ((*((volatile unsigned int *)(_mRegAddr))) & ~(0xC0 << shift)) | (uIntSourceType << shift);

    return 0;
}









 
INT32 sysSetLocalInterrupt(INT32 nIntState)
{
   INT32 temp;

   switch (nIntState)
   {
      case 0x7F:
      case 0xBF:
      case 0x3F:
           __asm
           {
               MRS    temp, CPSR
               AND    temp, temp, nIntState
               MSR    CPSR_c, temp
           }
           break;

      case 0x80:
      case 0x40:
      case 0xC0:
           __asm
           {
               MRS    temp, CPSR
               ORR    temp, temp, nIntState
               MSR    CPSR_c, temp
           }
           break;

      default:
           ;
   }
   return 0;
}

