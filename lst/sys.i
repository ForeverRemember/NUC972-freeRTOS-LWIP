INT32 sysGetPLL(UINT32 reg)
{
    UINT32 N,M,P;

    N =(((*((volatile unsigned int *)(reg))) & 0x007F)>>0)+1;
    M =(((*((volatile unsigned int *)(reg))) & 0x1F80)>>7)+1;
    P =(((*((volatile unsigned int *)(reg))) & 0xE000)>>13)+1;

    return (12*N/(M*P));     
}








 
UINT32 sysGetClock(CLK_Type clk)
{
    UINT32 src, divS, divN, reg, div;

    switch(clk)
    {
        case SYS_UPLL:
            return sysGetPLL((0xB0000200+0x64));

        case SYS_APLL:
            return sysGetPLL((0xB0000200+0x60));

        case SYS_SYSTEM:
        {
            reg = (*((volatile unsigned int *)((0xB0000200+0x20))));
            switch (reg & 0x18)
            {
                case 0x0:
                    src = 12;    
                    break;
                case 0x10:
                    src = sysGetPLL((0xB0000200+0x60));
                    break;
                case 0x18:
                    src = sysGetPLL((0xB0000200+0x64));
                    break;
                default:
                    return 0;
            }
            divS = (reg & 0x7) + 1;
            divN = ((reg & 0xf00) >> 8) + 1;
            return (src / divS / divN);
        }

        case SYS_HCLK1:
        {
            reg = (*((volatile unsigned int *)((0xB0000200+0x20))));
